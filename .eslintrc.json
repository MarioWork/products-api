{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["import", "prettier"],
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "error"
    }
}
